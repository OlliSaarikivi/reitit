<UserControl
    x:Name="ThisControl"
    x:Class="Reitit.FavIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Reitit"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400"
    Width="51.6666666666666" Height="51.6666666666666">
    
    <Border Background="{Binding Background, ElementName=ThisControl}">
        <Viewbox x:Name="Viewbox" Stretch="Uniform" Margin="5"/>
    </Border>
</UserControl>
